package mundial;

public class Partido {

	private  Pais pais1;
	private  Pais pais2;
	private double duracion;
	private int goles1;
	private int goles2;
	
	public Partido(Pais pais1, Pais pais2, double duracion, int goles1, int goles2) {
		super();
		this.pais1 = pais1;
		this.pais2 = pais2;
		this.duracion = duracion;
		this.goles1 = goles1;
		this.goles2 = goles2;
	}
}
